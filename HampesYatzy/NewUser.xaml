<Window x:Class="HampesYatzy.NewUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HampesYatzy"
        mc:Ignorable="d"
        WindowState="Maximized"
        WindowStyle="None"
        Title="Skapa användare" Height="900" Width="1800">
    <Grid Background="#FF389355" Margin="0,0,0.333,-6.667">
        <StackPanel Margin="0,0,0,847" Grid.ColumnSpan="2" >
            <Menu IsMainMenu="True" Height="43" Margin="0,0,0.333,0" RenderTransformOrigin="0.499,1.032">
                <MenuItem Header="Meny" Click="MenuItem_Click_1"></MenuItem>
                <MenuItem Header="Topplista" Click="MenuItem_Click"></MenuItem>
                <MenuItem Header="Skapa användare"></MenuItem>
            </Menu>
        </StackPanel>
        <TextBox x:Name="txtbox_firstname" HorizontalAlignment="Left" Height="32" Margin="50,168,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140"/>
        <TextBox x:Name="txtbox_lastname" HorizontalAlignment="Left" Height="32" Margin="46,340,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140"/>
        <Label x:Name="lastname" Content="Efternamn" HorizontalAlignment="Left" Margin="50,306,0,0" VerticalAlignment="Top" Width="101" FontSize="14"/>
        <Label x:Name="firstname" Content="Förnamn" HorizontalAlignment="Left" Margin="50,134,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label x:Name="username" Content="Användarnamn" Margin="50,472,260,386" Background="{x:Null}" FontSize="14"/>
        <Button x:Name="close_btn" Content="Stäng" HorizontalAlignment="Left" Margin="1216,0,0,0" VerticalAlignment="Top" Width="131" Height="43" Click="Kryss_btn_Click" Grid.Column="1"/>
        <TextBox x:Name="txtbox_username" Margin="50,514,1603,354" />
        <Button x:Name="button" Content="Skapa användare" HorizontalAlignment="Left" Margin="50,652,0,0" VerticalAlignment="Top" Width="208" Height="78" FontSize="18"/>
    </Grid>
</Window>
