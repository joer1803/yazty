<Window x:Class="HampesYatzy.NewUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HampesYatzy"
        mc:Ignorable="d"
        WindowState="Maximized"
        WindowStyle="None"
        Title="Skapa användare" Height="768" Width="1366">
    <Grid Background="#FF389355" Margin="0,0,0.333,-6.667">
        <StackPanel Margin="0,0,0,690">
            <Menu IsMainMenu="True" Height="49">
                <MenuItem Header="Meny" Click="MenuItem_Click_1" FontSize="22"/>
                <MenuItem Header="Topplista" Click="MenuItem_Click" FontSize="22"/>
                <MenuItem Header="Skapa användare" FontSize="22"/>
            </Menu>
        </StackPanel>
        <TextBox x:Name="txtbox_firstname" HorizontalAlignment="Left" Height="32" Margin="50,168,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140"/>
        <TextBox x:Name="txtbox_lastname" HorizontalAlignment="Left" Height="32" Margin="46,284,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140"/>
        <Label x:Name="lastname" Content="Efternamn" HorizontalAlignment="Left" Margin="50,250,0,0" VerticalAlignment="Top" Width="101" FontSize="14"/>
        <Label x:Name="firstname" Content="Förnamn" HorizontalAlignment="Left" Margin="50,134,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Button x:Name="close_btn" Content="Stäng" HorizontalAlignment="Left" Margin="1228,0,0,0" VerticalAlignment="Top" Width="131" Height="48" Click="Kryss_btn_Click"/>
        <Button x:Name="button" Content="Skapa användare" HorizontalAlignment="Left" Margin="46,523,0,0" VerticalAlignment="Top" Width="208" Height="78" FontSize="18"/>
        <Label x:Name="label" Content="Användarnamn" HorizontalAlignment="Left" Margin="46,385,0,0" VerticalAlignment="Top" Width="115" FontSize="14"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="32" Margin="46,419,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140"/>
    </Grid>
</Window>
